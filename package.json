{
  "name": "tg-io",
  "version": "1.11.2",
  "description": "Useful and fast library for Telegram Bots",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "watch": "tsc -p tsconfig.build.json --watch",
    "test": "jest --config jest.config.json --maxWorkers=1",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "build": "tsc -p tsconfig.build.json",
    "wDebug": "@powershell -Command $env:DEBUG='*';node lib/index.js",
    "uDebug": "DEBUG=* node lib/index.js"
  },
  "files": [
    "lib/*"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "homepage": "https://github.com/Ripretention/tg-io#readme",
  "bugs": "https://github.com/Ripretention/tg-io/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ripretention/tg-io.git"
  },
  "keywords": [
    "Telegram",
    "Telegram API",
    "Bot"
  ],
  "author": "Ripretention",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^18.7.21",
    "debug": "^4.3.4",
    "typescript": "^4.8.3",
    "undici": "^5.23.0"
  },
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.24.0",
    "jest": "^29.1.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.3"
  }
}
