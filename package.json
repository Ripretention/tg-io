{
	"name": "tg-io",
	"version": "1.0.0",
	"description": "Useful and fast library for Telegram Bots",
	"main": "index.js",
	"scripts": {
		"watch": "tsc -p tsconfig.build.json --watch",
		"test": "jest --config jest.config.json --maxWorkers=1",
		"lint": "eslint . --ext .ts",
		"lint-fix": "eslint . --ext .ts --fix",
		"start": "node dist/index.js",
		"build": "tsc -p tsconfig.build.json",
		"postinstall": "tsc -p tsconfig.build.json",
		"wDebug": "@powershell -Command $env:DEBUG='*';node dist/index.js",
		"uDebug": "DEBUG=* node dist/index.js"
	},
	"keywords": [
		"Telegram",
		"Telegram API",
		"Bot"
	],
	"author": "Ripretention",
	"license": "ISC",
	"dependencies": {
		"@types/node": "^18.7.21",
		"axios": "^0.27.2",
		"debug": "^4.3.4",
		"typescript": "^4.8.3"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"eslint": "^8.24.0"
	}
}
